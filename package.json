{
  "name": "RNApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:8": "react-native run-ios --simulator=\"iPhone 8\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "ignore": [
      "node_modules/**"
    ],
    "linters": {
      "*.{ts,tsx}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.{js,jsx}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.less": [
        "stylelint --syntax less",
        "prettier --write",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/react-native": "^3.2.0",
    "@react-native-community/netinfo": "^5.2.0",
    "@types/lodash": "^4.14.149",
    "@types/qs": "^6.9.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.5",
    "axios": "^0.19.0",
    "dva-core": "^2.0.2",
    "dva-immer": "^0.4.19",
    "dva-loading": "^3.0.20",
    "dva-model-extend": "^0.1.2",
    "lodash": "^4.17.15",
    "qs": "^6.9.1",
    "react": "16.11.0",
    "react-native": "0.62.3",
    "react-native-cached-image": "git+https://github.com/fungilation/react-native-cached-image.git",
    "react-native-device-info": "^5.4.0",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-extra-dimensions-android": "^1.2.5",
    "react-native-fit-image": "^1.5.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-progress": "^4.0.3",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-size-matters": "^0.3.0",
    "react-native-spinkit": "^1.5.0",
    "react-native-tab-view": "^2.11.0",
    "react-native-text": "^0.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-header-buttons": "^3.0.4",
    "react-navigation-props-mapper": "^1.0.1",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.6.2",
    "react-redux": "^7.1.3",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/runtime": "^7.6.2",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/mockjs": "^1.0.2",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^7.2.2",
    "metro-react-native-babel-preset": "^0.58.0",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
